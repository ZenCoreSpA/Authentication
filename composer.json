{
    "name": "zencorespa/authentication",
    "description": "Package to provide authentication",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZenCoreSpA\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZenCoreSpA\\Authentication\\Tests\\": "tests",
            "ZenCoreSpA\\Authentication\\Database\\Factories\\": "database/factories"
        }
    },
    "authors": [
        {
            "name": "Ian Torres",
            "email": "iantorres@outlook.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ZenCoreSpA\\Authentication\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "zencorespa/ui": "*",
        "illuminate/routing": "^9",
        "illuminate/session": "^9",
        "illuminate/support": "^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "orchestra/testbench": "^7.9"
    },
    "scripts": {
        "test": "phpunit"
    }
}
